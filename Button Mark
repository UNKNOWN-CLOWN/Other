local ScreenGui = Instance.new("ScreenGui")
local VIRTUAL = Instance.new("ImageButton")
ScreenGui.Parent = game.CoreGui
ScreenGui.ResetOnSpawn = false
VIRTUAL.Parent = ScreenGui
VIRTUAL.BackgroundColor3 = Color3.fromRGB(20,10,1)
VIRTUAL.BackgroundTransparency = 1.000
VIRTUAL.Position = UDim2.new(0.21,       0, 0.92,         0)
VIRTUAL.Size = UDim2.new(0, 35, 0, 35)
VIRTUAL.Image = "rbxassetid://9210647432"
VIRTUAL.ImageColor3 = Color3.fromRGB(255,255,255)
VIRTUAL.AnchorPoint = Vector2.new(0, 0.060)
VIRTUAL.MouseButton1Click:Connect(function()
    game:GetService('VirtualInputManager'):SendKeyEvent(true, "LeftAlt", false, game)
end)
